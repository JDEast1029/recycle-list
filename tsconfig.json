{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "resolveJsonModule": true,
    "noImplicitAny": false,  // 在表达式和声明上有隐含的 any类型时是否报错。
  },
  "include": ["src/**/*", "dev/**/*"],
  "exclude": [
    "node_modules",
    "examples/**/*"
  ]
}
